    "prettier": "^3.2.5",
    "sass": "^1.77.2",
    "simple-git-hooks": "^2.11.1",
    "svg-country-flags": "^1.2.10"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
